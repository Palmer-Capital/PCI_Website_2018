@{
    ViewData["Title"] = "Palmer Capital Team";  //shows in title in jumbotron
    ViewData["SubTitle"] = "All Team Members"; //shows in h4 in jumbotron
}

@section Scroll
{
    <style type="text/css">
        html { overflow-y: scroll; }
    </style>
}

    @await Html.PartialAsync("_FirmJumbo")
    @await Html.PartialAsync("_Firm")
    @await Html.PartialAsync("_QuickLinks")

    </div>
</div>

@section Top {
    <div class="top" title="scroll to top">
    <button type="button" class="btn btn-sm btn-default">
        <i class="material-icons icon-lg">arrow_upward</i>
    </button>
    </div>
}

@section scripts {
    <environment include="Development">
        <script src="~/js/firm.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/firm.js" asp-append-version="true"></script>
    </environment>
}